@if(string.IsNullOrEmpty(Model.City) || string.IsNullOrEmpty(Model.Name))
{
<form method="get">
	<div class="input-group1">
		<select asp-for="IzabranaVrsta" asp-items="Model.Lista" class="selectPretraga" id="inputGroupSelect04" aria-label="Example select with button addon">
			<option value="0">All</option>
		</select>
		<div class="input-group-append">
    		<button class="btnPretraga" type="submit">Search</button>
  		</div>
	</div>


	<input type="hidden" asp-for="City">
	<input type="hidden" asp-for="Name">
</form>
}

<div class="Popular-Restaurants-content">
		<div class="Popular-Restaurants-grids">
			<div class="container">

			@for(int i=0; i<Model.VrsteObjekata.Count(); i++)
			{
				<h3 class="r-titles">@Model.VrsteObjekata[i]</h3>

				@foreach (var lok in Model.MatricaLokala[i])
				{
					<div class="Popular-Restaurants-grid wow fadeInRight" data-wow-delay="0.4s">
					<div class="col-md-3 restaurent-logo">
						<img style="height:250px; width:250px;"src=@lok.nazivSlike class="img-responsive" alt="" />
					</div>
					<div class="col-md-2 restaurent-title">
						<div class="logo-title">
							<h4><a asp-page="/Object" asp-route-id=@lok.Id>@lok.Naziv</a></h4>
						</div>
						<div class="rating">
							<label>City: @lok.Grad</label>
							<div><label>Rating: </label>
							<label>@lok.Ocena</label> <label>(@lok.brOcena)</label></div>
							<div><label>Open: @lok.openTime.ToString("h tt")</label> </div>
							<label>Close: @lok.closeTime.ToString("h tt")</label>
						</div>
					</div>
					<div class="col-md-7 buy">
						<a class="morebtn hvr-rectangle-in" asp-page="/Object" asp-route-Id="@lok.Id">Reserve</a>
					</div>
					<div class="clearfix"></div>
				</div>
				}
			}
				
			<!--<a href="#"> <img src="images/star1.png" class="img-responsive" alt="">(004)</a>-->
		</div>
	</div>
